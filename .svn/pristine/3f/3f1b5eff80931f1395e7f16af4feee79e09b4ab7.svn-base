    private DataTable DataByWorksite(DataTable dt, string worksitename)
    {
        DataTable dtjson = dt.Clone();
        DataRow[] rows = dt.Select("worksiteid='" + worksitename + "'");

        foreach (DataRow row in rows)
        {

            dtjson.Rows.Add(row.ItemArray);
        }
        return dtjson;

    }
